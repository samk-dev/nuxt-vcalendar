{
  "name": "@samk-dev/nuxt-vcalendar",
  "version": "1.0.0-dev",
  "description": "Integrates V Calendar in Nuxt",
  "repository": "samk-dev/nuxt-vcalendar",
  "license": "MIT",
  "author": {
    "name": "Sam K",
    "url": "https://samk.dev"
  },
  "bugs": "https://github.com/samk-dev/nuxt-uikit3/issues",
  "keywords": [
    "nuxt-module",
    "vcalendar",
    "vue calendar component",
    "nuxt calendar component"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,vue,json}\"",
    "prepare": "husky install",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.5.2",
    "@popperjs/core": "^2.11.8",
    "v-calendar": "^3.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@nuxt/module-builder": "^0.4.0",
    "@nuxt/schema": "^3.5.2",
    "@nuxt/test-utils": "^3.5.1",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/node": "^18",
    "changelogen": "^0.5.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nuxt": "^3.5.3",
    "prettier": "^2.8.8",
    "vitest": "^0.32.2"
  },
  "lint-staged": {
    "**/*.{.js,ts,vue,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
